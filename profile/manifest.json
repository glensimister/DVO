{
    "name": "DVO Profile",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Public profile for DVO",
    "permissions": ["contextMenus"],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": { // optional
            "16": "icons/icon16.png", // optional
            "24": "icons/icon48.png", // optional
            "32": "icons/icon128.png" // optional
        },
        "default_title": "DVO Profile", // optional; shown in tooltip
        "default_popup": "index.html" // optional
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "web_accessible_resources": [
       "images/*",
        "css/*",
        "fonts/*",
        "js/*"
     ]
}
